.grid {
  display: flex;
  flex-wrap: wrap;
  margin-left: -{{ grid_gutter }}px;
  padding: 0;
  list-style: none;
}

.grid--row-gutters {
  margin-bottom: -{{ content_row_padding }}px;
}

.grid--row-gutters-small {
  margin-bottom: -{{ grid_gutter }}px;
}

.column {
  margin-left: {{ grid_gutter }}px;
}
.grid--row-gutters > .column {
  margin-bottom: {{ content_row_padding }}px;
}
.grid--row-gutters-small > .column {
  margin-bottom: {{ grid_gutter }}px;
}

.column.half {
  width: calc(50% - {{ grid_gutter }}px);
}

.column.quarter {
  width: calc(25% - {{ grid_gutter }}px);
}

.column.third {
  width: calc(33.33333% - {{ grid_gutter }}px);
}

.column.fifth {
  width: calc(20% - {{ grid_gutter }}px);
}

.column.full {
  width: calc(100% - {{ grid_gutter }}px);
}

@media (max-width: 767px) {
  body > .container {
    width: auto;
  }

  .grid {
    margin: 0 0 -{{ grid_gutter_mobile }}px -{{ grid_gutter_mobile }}px;
  }

  .grid--row-gutters {
    margin-bottom: -{{ grid_gutter_mobile | times: 2 }}px;
  }

  .grid--row-gutters-small {
    margin-bottom: -{{ grid_gutter_mobile }}px;
